---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Example Feed</title>
  <link href="{{- site.url -}}{{- site.baseurl -}}/blog-business/feed.xml" rel="self" type="application/atom+xml"/>
  <link href="{{- site.url -}}{{- site.baseurl -}}/blog-business/feed.xml" rel="alternate" type="text/html"/>
  <updated>{{- site.time | date_to_xmlschema -}}</updated>
  <id>{{- site.url -}}{{- site.baseurl -}}/blog-business/feed.xml</id>
  <title>{{site.title}} Business Blog></title>
  <subtitle>{{- site.description | strip_html | lstrip | rstrip -}}</subtitle>
  <author>
    <name>
    {%- if site.author -%}
        {{- site.author -}}
    {%- else -%}
        {{- site.title -}}
    {%- endif -%}
    </name>
  </author>
</feed>